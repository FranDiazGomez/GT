{
  "media": {
    "phone": "screen and (min-width: 0px) and (max-width: 480px)",
    "tablet": "screen and (min-width: 481px) and (max-width: 840px)",
    "desktop": "screen and (min-width: 841px) and (max-width: 1280px)",
    "large-desktop": "screen and (min-width: 1281px)"
  },

  "styles": {
    "controls": {

      "VisiWin.UI.CardLayout": {
        "media": {
          "phone": {
            "deviceType": "isPhone",
            "columnWidth": "25%"
          },
          "tablet": {
            "deviceType": "isTablet",
            "columnWidth": "12.5%"
          },
          "desktop": {
            "deviceType": "isDesktop",
            "columnWidth": "8.3333333333%"
          },
          "large-desktop": {
            "deviceType": "isLargeDesktop",
            "columnWidth": "6.25%"
          }
        }
      },

      "VisiWin.UI.CardLayout.Lenze.Default": {
