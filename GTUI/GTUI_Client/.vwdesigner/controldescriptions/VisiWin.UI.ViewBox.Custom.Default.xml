<?xml version="1.0" encoding="utf-8"?>
<control version="7.2" spec="1.0" runtimeType="VisiWin.UI.ViewBox" style="Custom.Default" name="VisiWin.UI.ViewBox.Custom.Default">
	<author name="Custom" />
	<title type="text/plain">ViewBox</title>
	<description type="text/plain">A ViewBox control.</description>
	<attachedProperties>
		<attachedProperty name="left" isBrowsable="true"/>
		<attachedProperty name="top" isBrowsable="true"/>
		<attachedProperty name="right" isBrowsable="true"/>
		<attachedProperty name="bottom" isBrowsable="true"/>
		<attachedProperty name="margin" isBrowsable="false"/>
		<attachedProperty name="flexShrink" isBrowsable="false"/>
		<attachedProperty name="flexGrow" isBrowsable="false"/>
		<attachedProperty name="flexBasis" isBrowsable="false"/>
	</attachedProperties>
	<properties>
		<!-- Layout -->
		<property displayName="Width" name="width" datatype="String" category="Layout" isBrowsable="true" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="WidthUnits">
			<description type="text/plain">Gets or sets the Width option.</description>
		</property>
		<property displayName="Min width" name="minWidth" datatype="String" category="Layout" isBrowsable="false" isAdvanced="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="MinWidthUnits">
			<description type="text/plain">Gets or sets the MinWidth option.</description>
		</property>
		<property displayName="Max width" name="maxWidth" datatype="String" category="Layout" isBrowsable="false" isAdvanced="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="MaxWidthUnits">
			<description type="text/plain">Gets or sets the MaxWidth option.</description>
		</property>
		<property displayName="Height" name="height" datatype="String" category="Layout" isBrowsable="true" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="HeightUnits">
			<description type="text/plain">Gets or sets the Height option.</description>
		</property>
		<property displayName="Min height" name="minHeight" datatype="String" category="Layout" isBrowsable="false" isAdvanced="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="MinHeightUnits">
			<description type="text/plain">Gets or sets the MinHeight option.</description>
		</property>
		<property displayName="Max height" name="maxHeight" datatype="String" category="Layout" isBrowsable="false" isAdvanced="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="MaxHeightUnits">
			<description type="text/plain">Gets or sets the MaxHeight option.</description>
		</property>
		<property displayName="Padding" name="padding" datatype="String" category="Layout" isBrowsable="false" isAdvanced="false" editor="HTML5Design.DesignInformation.ThicknessPropertyEditor">
			<description type="text/plain">Gets or sets the Padding option.</description>
		</property>
		<property displayName="Flex shrink" name="flexShrink" datatype="Number" category="Layout" isBrowsable="false" isAdvanced="false" isInteger="true">
			<description type="text/plain">Gets or sets the FlexShrink option.</description>
		</property>

		<!-- Appearance -->
		<property displayName="Border width" name="borderWidth" datatype="String" category="Appearance" isBrowsable="false" editor="HTML5Design.DesignInformation.ThicknessPropertyEditor">
			<description type="text/plain">Gets or sets the BorderWidth option.</description>
		</property>
		<property displayName="Border style" name="borderStyle" datatype="VisiWin.UI.Border.Style" category="Appearance" isBrowsable="false">
			<description type="text/plain">Gets or sets the BorderStyle option.</description>
		</property>
		<property displayName="Border radius" name="borderRadius" datatype="String" category="Appearance" isBrowsable="false" editor="HTML5Design.DesignInformation.ThicknessPropertyEditor">
			<description type="text/plain">Gets or sets the BorderRadius option.</description>
		</property>
		<property displayName="Opacity" name="opacity" datatype="Number" category="Appearance" isBrowsable="false">
			<description type="text/plain">Gets or sets the Opacity option.</description>
		</property>
		<property displayName="Visibility" name="visibility" datatype="VisiWin.UI.Visibility" category="Appearance" isBrowsable="false">
			<description type="text/plain">Gets or sets the Visibility option.</description>
		</property>
		<property displayName="Shadow" name="shadow" datatype="String" category="Appearance" isBrowsable="false" editor="HTML5Design.DesignInformation.ShadowPropertyEditor">
			<description type="text/plain">Gets or sets the Shadow option.</description>
		</property>
		<property displayName="Background" name="background" datatype="String" category="Appearance" isBrowsable="false" editor="HTML5Design.DesignInformation.ColorPropertyEditor" editorParameter="background">
			<description type="text/plain">Gets or sets the Background option.</description>
		</property>
		<property displayName="Style" name="style" datatype="String" category="Appearance" isBrowsable="true" editor="HTML5Design.DesignInformation.StylePropertyEditor" refreshRequired="true">
			<description type="text/plain">Gets or sets the Style option.</description>
		</property>

		<!-- Common Properties -->

		<property displayName="Class names" name="classNames" datatype="String" category="Common Properties" isBrowsable="false">
			<description type="text/plain">Gets or sets the ClassNames option.</description>
		</property>
		<property displayName="Data context" name="dataContext" datatype="String" category="Common Properties" isBrowsable="false" editor="HTML5Design.DesignInformation.ObservableDialogPropertyEditor">
			<description type="text/plain">Gets or sets the DataContext option.</description>
		</property>

		<!-- Colors -->
		<property displayName="Background color" name="backgroundColor" datatype="String" category="Colors" isBrowsable="false" editor="HTML5Design.DesignInformation.ColorPropertyEditor">
			<description type="text/plain">Gets or sets the BackgroundColor option.</description>
		</property>
		<property displayName="Border color" name="borderColor" datatype="String" category="Colors" isBrowsable="false" editor="HTML5Design.DesignInformation.ColorPropertyEditor">
			<description type="text/plain">Gets or sets the BorderColor option.</description>
		</property>

		<!-- ViewBox -->
		<property displayName="Stretch mode" name="stretchMode" datatype="VisiWin.UI.ViewBox.StretchMode" category="ViewBox" isBrowsable="true">
			<description type="text/plain">Describes how the content is scaled to fill the entire allocated space.</description>
		</property>
		<property displayName="Stretch direction" name="stretchDirection" datatype="VisiWin.UI.ViewBox.StretchDirection" category="ViewBox" isBrowsable="true">
			<description type="text/plain">StretchDirection sets the scaling constraints that apply to the contents of the ViewBox. Use this property to prevent the contents of the ViewBox from being smaller or larger than its original size.</description>
		</property>
		<property displayName="Overflow" name="overFlow" datatype="VisiWin.UI.Overflow" category="ViewBox" isBrowsable="false">
			<description type="text/plain">Gets or sets the BorderColor option.</description>
		</property>
		<property displayName="Default device width" name="defaultDeviceWidth" datatype="String" category="ViewBox" isBrowsable="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="FlexBasisUnits">
			<description type="text/plain">Gets or sets the Width option.</description>
		</property>
		<property displayName="Default device height" name="defaultDeviceHeight" datatype="String" category="ViewBox" isBrowsable="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="FlexBasisUnits">
			<description type="text/plain">Gets or sets the Width option.</description>
		</property>
		<property displayName="Stretch in design mode" name="stretchInDesignMode" datatype="Boolean" category="ViewBox" isBrowsable="false" refreshRequired="true">
			<description type="text/plain">Gets or sets the Width option.</description>
		</property>
		<property displayName="Stretch anchor" name="stretchAnchor" datatype="VisiWin.UI.ViewBox.StretchAnchor" category="ViewBox" isBrowsable="false">
			<description type="text/plain">Gets or sets the Width option.</description>
		</property>



	</properties>

	<!-- Events -->
	<events>
		<event name="onInitialized"/>
		<event name="onAttached"/>
		<event name="onDetached"/>
		<event name="onVisibilityChanged"/>

		<event name="onClick"/>
		<event name="onDown"/>
		<event name="onUp"/>

	</events>

	<content>
		<![CDATA[<div data-win-control="VisiWin.UI.ViewBox" data-win-options='{"style":"Custom.Default","left":"0px","top":"0px","width":"100%","height":"100%"}'></div>]]>
	</content>
</control>
