<?xml version="1.0" encoding="utf-8"?>
<control version="7.2" spec="1.0" runtimeType="VisiWin.UI.CardLayout" style="Custom.Default" name="VisiWin.UI.CardLayout.Custom.Default">
  <author name="Custom" />
  <title type="text/plain">CardLayout</title>
  <description type="text/plain">A CardLayout control.</description>
    <attachedProperties>
    <attachedProperty name="left" isBrowsable="false"/>
    <attachedProperty name="top" isBrowsable="false"/>
    <attachedProperty name="right" isBrowsable="false"/>
    <attachedProperty name="bottom" isBrowsable="false"/>
    <attachedProperty name="margin" isBrowsable="false"/>
    <attachedProperty name="flexShrink" isBrowsable="true"/>
    <attachedProperty name="flexGrow" isBrowsable="true"/>
    <attachedProperty name="flexBasis" isBrowsable="true"/>
    <attachedProperty name="minWidth" isBrowsable="true"/>
    <attachedProperty name="maxWidth" isBrowsable="true"/>
    <attachedProperty name="minHeight" isBrowsable="true"/>
    <attachedProperty name="maxHeight" isBrowsable="true"/>
    <attachedProperty name="padding" isBrowsable="false"/>
  </attachedProperties>
  <properties>
    <!-- Layout -->
    <property displayName="Width" name="width" datatype="String" category="Layout" isBrowsable="true" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="WidthUnits">
      <description type="text/plain">Gets or sets the Width option.</description>
    </property>
    <property displayName="Min width" name="minWidth" datatype="String" category="Layout" isBrowsable="true" isAdvanced="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="MinWidthUnits">
      <description type="text/plain">Gets or sets the MinWidth option.</description>
    </property>
    <property displayName="Max width" name="maxWidth" datatype="String" category="Layout" isBrowsable="true" isAdvanced="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="MaxWidthUnits">
      <description type="text/plain">Gets or sets the MaxWidth option.</description>
    </property>
    <property displayName="Height" name="height" datatype="String" category="Layout" isBrowsable="true" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="HeightUnits">
      <description type="text/plain">Gets or sets the Height option.</description>
    </property>
    <property displayName="Min height" name="minHeight" datatype="String" category="Layout" isBrowsable="true" isAdvanced="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="MinHeightUnits">
      <description type="text/plain">Gets or sets the MinHeight option.</description>
    </property>
    <property displayName="Max height" name="maxHeight" datatype="String" category="Layout" isBrowsable="true" isAdvanced="false" editor="HTML5Design.DesignInformation.SizePropertyEditor" editorParameter="MaxHeightUnits">
      <description type="text/plain">Gets or sets the MaxHeight option.</description>
    </property>
    <property displayName="Overflow" name="overflow" datatype="VisiWin.UI.Overflow" category="Layout" isBrowsable="true" isAdvanced="false">
			<description type="text/plain">Gets or sets the Overflow option.</description>
		</property>

    <!-- Appearance -->
    <property displayName="Border width" name="borderWidth" datatype="String" category="Appearance" isBrowsable="true" editor="HTML5Design.DesignInformation.ThicknessPropertyEditor">
      <description type="text/plain">Gets or sets the BorderWidth option.</description>
    </property>
    <property displayName="Border style" name="borderStyle" datatype="VisiWin.UI.Border.Style" category="Appearance" isBrowsable="true">
      <description type="text/plain">Gets or sets the BorderStyle option.</description>
    </property>
    <property displayName="Border radius" name="borderRadius" datatype="String" category="Appearance" isBrowsable="true" editor="HTML5Design.DesignInformation.ThicknessPropertyEditor">
      <description type="text/plain">Gets or sets the BorderRadius option.</description>
    </property>
    <property displayName="Opacity" name="opacity" datatype="Number" category="Appearance" isBrowsable="true">
      <description type="text/plain">Gets or sets the Opacity option.</description>
    </property>
    <property displayName="Visibility" name="visibility" datatype="VisiWin.UI.Visibility" category="Appearance" isBrowsable="true">
      <description type="text/plain">Gets or sets the Visibility option.</description>
    </property>
    <property displayName="Shadow" name="shadow" datatype="String" category="Appearance" isBrowsable="true" editor="HTML5Design.DesignInformation.ShadowPropertyEditor">
      <description type="text/plain">Gets or sets the Shadow option.</description>
    </property>
    <property displayName="Background" name="background" datatype="String" category="Appearance" isBrowsable="true" editor="HTML5Design.DesignInformation.ColorPropertyEditor" editorParameter="background">
      <description type="text/plain">Gets or sets the Background option.</description>
    </property>

    <!-- Common Properties -->
    <property displayName="Style" name="style" datatype="String" category="Common Properties" isBrowsable="true" editor="HTML5Design.DesignInformation.StylePropertyEditor" refreshRequired="true">
      <description type="text/plain">Gets or sets the Style option.</description>
    </property>
    <property displayName="Class names" name="classNames" datatype="String" category="Common Properties" isBrowsable="true">
      <description type="text/plain">Gets or sets the ClassNames option.</description>
    </property>
    <property displayName="Data context" name="dataContext" datatype="String" category="Common Properties" isBrowsable="true" editor="HTML5Design.DesignInformation.ObservableDialogPropertyEditor">
      <description type="text/plain">Gets or sets the DataContext option.</description>
    </property>
    <property displayName="Scroll y offset" name="scrollYOffset" datatype="Number" category="Common Properties" isBrowsable="true" isInteger="true">
      <description type="text/plain">>Gets or sets the ScrollYOffset option.</description>
    </property>

    <!-- Gutter -->
    <property displayName="Gutter phone" name="gutterPhone" datatype="String" category="Gutter" isBrowsable="true">
      <description type="text/plain">Gets or sets the GutterPhone option.</description>
    </property>
    <property displayName="Gutter tablet"  name="gutterTablet" datatype="String" category="Gutter" isBrowsable="true">
      <description type="text/plain">Gets or sets the GutterTablet option.</description>
    </property>
    <property displayName="Gutter desktop"  name="gutterDesktop" datatype="String" category="Gutter" isBrowsable="true">
      <description type="text/plain">Gets or sets the GutterDesktop option.</description>
    </property>
    <property displayName="Gutter large desktop"  name="gutterLargeDesktop" datatype="String" category="Gutter" isBrowsable="true">
      <description type="text/plain">Gets or sets the GutterLargeDesktop option.</description>
    </property>

    <!-- Padding -->
    <property displayName="Padding phone" name="paddingPhone" datatype="String" category="Padding" isBrowsable="true">
      <description type="text/plain">Gets or sets the PaddingPhone option.</description>
    </property>
    <property displayName="Padding tablet" name="paddingTablet" datatype="String" category="Padding" isBrowsable="true">
      <description type="text/plain">Gets or sets the PaddingTablet option.</description>
    </property>
    <property displayName="Padding desktop" name="paddingDesktop" datatype="String" category="Padding" isBrowsable="true">
      <description type="text/plain">Gets or sets the PaddingDesktop option.</description>
    </property>
    <property displayName="Padding large desktop" name="paddingLargeDesktop" datatype="String" category="Padding" isBrowsable="true">
      <description type="text/plain">Gets or sets the PaddingLargeDesktop option.</description>
    </property>

    <!-- Authorization -->
    <property displayName="Authorization mode" name="authorizationMode" datatype="VisiWin.UI.Authorization.Mode" category="Authorization" isBrowsable="true">
      <description type="text/plain">Gets or sets the AuthorizationMode option.</description>
    </property>
    <property displayName="Authorization right" name="authorizationRight" datatype="String" category="Authorization" isBrowsable="true" editor="HTML5Design.DesignInformation.RightDialogPropertyEditor">
      <description type="text/plain">Gets or sets the AuthorizationRight option.</description>
    </property>
    <property displayName="Authorized (design time)" name="authorized" datatype="Boolean" category="Control States" isBrowsable="false" shouldSerialize="false">
      <description type="text/plain">Gets or sets the Authorized option.</description>
    </property>

    <!-- Colors -->
    <property displayName="Background color" name="backgroundColor" datatype="String" category="Colors" isBrowsable="true" editor="HTML5Design.DesignInformation.ColorPropertyEditor">
      <description type="text/plain">Gets or sets the BackgroundColor option.</description>
    </property>
    <property displayName="Border color" name="borderColor" datatype="String" category="Colors" isBrowsable="true" editor="HTML5Design.DesignInformation.ColorPropertyEditor">
      <description type="text/plain">Gets or sets the BorderColor option.</description>
    </property>
  </properties>

  <!-- Events -->
  <events>
    <event name="onInitialized"/>
    <event name="onAttached"/>
    <event name="onDetached"/>
    <event name="onVisibilityChanged"/>
  </events>

  <content>
    <![CDATA[<div data-win-control="VisiWin.UI.CardLayout" data-win-options="{style:'Custom.Default'}"></div>]]>
  </content>
</control>
